{
	"name": "Twitch Tools",
	"description": "Twitch.tv utilities.",
	"baseFile": "plugin.iced",
	"hidden": false,
	"handlers": {
		"twitch.followcheck": {
			"description": "Checks the status of a follow relationship.",
			"settings": {
				"format": {
					"name": "Message Format",
					"description": "Message format.",
					"default": "<%displayName%> is following for <%countdown/dateFollowed%>.",
					"type": "format",
					"variables": {
						"dateFollowed": "Date followed",
						"displayName": "Display name"
					}
				}
			}
		}
	}
}